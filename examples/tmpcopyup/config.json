{
    "ociVersion": "1.0.2",
    "process": {
        "cwd": "/",
        "env": ["PATH=/rescue"],
        "args": ["cat", "/tmpdir/file"]
    },
    "root": {
        "readonly": true
    },
    "mounts": [
        {
            "type": "tmpfs",
            "destination": "/tmpdir",
            "options": ["tmpcopyup"]
        }
    ]
}
